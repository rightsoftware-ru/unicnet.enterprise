digraph Dependencies {
    rankdir=TB;
    node [shape=box, style="rounded,filled", fontname="Arial", fontsize=12, fontcolor=white];
    edge [color=black, style=bold];
    
    postgres [label="unicnet.postgres", fillcolor="#1e88e5", color="#0d47a1"];
    keycloak [label="unicnet.keycloak", fillcolor="#fb8c00", color="#e65100"];
    postgres -> keycloak;
    vault [label="unicnet.vault", fillcolor="#7b1fa2", color="#4a148c"];
    backend [label="unicnet.backend", fillcolor="#e53935", color="#b71c1c"];
    vault -> backend;
    router [label="unicnet.router", fillcolor="#00897b", color="#004d40"];
    router -> backend;
    syslog [label="unicnet.syslog", fillcolor="#616161", color="#212121"];
    syslog -> backend;
    logger [label="unicnet.logger", fillcolor="#616161", color="#212121"];
    logger -> backend;
    frontend [label="unicnet.frontend", fillcolor="#d81b60", color="#880e4f"];
    backend -> frontend;
    keycloak -> frontend;
    mongo [label="unicnet.mongo", fillcolor="#43a047", color="#1b5e20"];
    mongo -> logger;
    mongo -> vault;
    logger -> vault;
    mongo -> syslog;
    vault -> syslog;
}
